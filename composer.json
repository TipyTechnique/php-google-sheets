{
  "name": "tipytechnique/php-google-sheets",
  "description": "Google Sheets API v4 (with Laravel or simple PHP)",
  "keywords": [
    "google",
    "sheets",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.3",
    "illuminate/container": "^5.8",
    "pulkitjalan/google-apiclient": "^3.1",
    "vlucas/phpdotenv": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "~3.8"
  },
  "autoload": {
    "psr-4": {
      "Tipy\\Google\\Sheets\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "technique",
      "email": "technique@tipy.tv"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Tipy\\Google\\Sheets\\Providers\\SheetsServiceProvider"
      ],
      "aliases": {
        "Sheets": "Tipy\\Google\\Sheets\\Facades\\Sheets"
      }
    }
  }
}
